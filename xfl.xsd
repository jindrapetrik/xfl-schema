                                        <xs:annotation>
                                            <xs:documentation><![CDATA[ 
                                                <p>Export format of the sound.</p>
                                                <h2>WAV</h2>
                                                <ul>
                                                    <li>stereo +1</li>
                                                    <li>5.5kHz +2</li>
                                                    <li>11kHz  +6</li>
                                                    <li>22kHz  +10</li>
                                                    <li>44kHz  +14</li>
                                                </ul>
                                                <h2>MP3</h2>
                                                <ul>
                                                    <li>stereo +1</li>
                                                    <li>bestQuality +4</li>
                                                </ul>
                                                ]]>                                                
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="exportBits" type="xs:integer" use="optional">
