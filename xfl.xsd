                    <xs:attribute name="saturation" use="optional" default="0.0">
                        <xs:annotation>
                            <xs:documentation>Saturation adjustment value. +-percentage</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="-100"/>
                                <xs:maxInclusive value="100"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="hue" use="optional" default="0.0">
                        <xs:annotation>
                            <xs:documentation>Hue adjustment value. -180 to 180 degrees</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="-180"/>
                                <xs:maxInclusive value="180"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    
                    <xs:attributeGroup ref="tns:FilterAttrs"/>
                </xs:complexType>
            </xs:element>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="tElementList">
        <xs:annotation>
            <xs:documentation>List of elements on the display.</xs:documentation>
        </xs:annotation>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="DOMGroup">
                <xs:annotation>
                    <xs:documentation>Group of display objects.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="matrix" type="tns:tMatrix" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation matrix for the group.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="transformationPoint" type="tns:tPoint" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation point for the group.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="members" type="tns:tElementList" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Members of the group.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="DOMSymbolInstance">
                <xs:annotation>
                    <xs:documentation>Instance of a symbol from the library.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="matrix" type="tns:tMatrix" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation matrix for the symbol instance.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="transformationPoint" type="tns:tPoint" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation point for the symbol instance.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Actionscript" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>ActionScript code associated with the symbol instance.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="script" type="xs:string"/>                                                                                                    
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="filters" minOccurs="0" maxOccurs="1" type="tns:tFilterList">
                            <xs:annotation>
                                <xs:documentation>List of filters applied to the symbol instance.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="color" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Color transformation applied to the symbol instance.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Color">
                                        <xs:complexType>
                                            <xs:attribute name="redMultiplier" type="xs:float" use="optional" default="1.0">
                                                <xs:annotation>
                                                    <xs:documentation>Value to multiply red component with.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="greenMultiplier" type="xs:float" use="optional" default="1.0">
                                                <xs:annotation>
                                                    <xs:documentation>Value to multiply green component with.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="blueMultiplier" type="xs:float" use="optional" default="1.0">
                                                <xs:annotation>
                                                    <xs:documentation>Value to multiply blue component with.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="alphaMultiplier" type="xs:float" use="optional" default="1.0">
                                                <xs:annotation>
                                                    <xs:documentation>Value to multiply alpha component with.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="redOffset" type="xs:integer" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Value to add to red component.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="greenOffset" type="xs:integer" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Value to add to green component.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="blueOffset" type="xs:integer" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Value to add to blue component.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="alphaOffset" type="xs:integer" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Value to add to alpha component.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>

                                            <xs:attribute name="brightness" use="optional">
                                                <xs:annotation>
                                                    <xs:documentation>Brightness adjustment value. -1 to 1</xs:documentation>
                                                </xs:annotation>
                                                <xs:simpleType>
                                                    <xs:restriction base="xs:float">
                                                        <xs:minInclusive value="-1"/>
                                                        <xs:maxInclusive value="1"/>
                                                    </xs:restriction>
                                                </xs:simpleType>
                                            </xs:attribute>
                                            <xs:attribute name="tintColor" type="tns:tColor" use="optional">
                                                <xs:annotation>
                                                    <xs:documentation>Tint color value.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="tintMultiplier" use="optional">
                                                <xs:annotation>
                                                    <xs:documentation>Amount of tint to apply. 0 to 1</xs:documentation>
                                                </xs:annotation>
                                                <xs:simpleType>
                                                    <xs:restriction base="xs:float">
                                                        <xs:minInclusive value="0"/>
                                                        <xs:maxInclusive value="1"/>
                                                    </xs:restriction>
                                                </xs:simpleType>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="MatteColor" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Background color used when caching as bitmap with opaque setting.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:attribute name="color" type="tns:tColor" use="optional" default="#ffffff"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="libraryItemName" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Name of the symbol in the library.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="symbolType" type="tns:tSymbolType" use="optional" default="movie clip">
                        <xs:annotation>
                            <xs:documentation>Type of the symbol instance.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="centerPoint3DX" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>X coordinate of the 3D center point.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="centerPoint3DY" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>Y coordinate of the 3D center point.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="centerPoint3DZ" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>Z coordinate of the 3D center point.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="loop" use="optional" default="loop">
                        <xs:annotation>
                            <xs:documentation>If symbolType=graphic: Whether to loop.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="loop">
                                    <xs:annotation>
                                        <xs:documentation>Loop through the frames indefinitely.</xs:documentation>
                                    </xs:annotation>
                                </xs:enumeration>
                                <xs:enumeration value="play once">
                                    <xs:annotation>
                                        <xs:documentation>Play only once through the frames.</xs:documentation>
                                    </xs:annotation>
                                </xs:enumeration>
                                <xs:enumeration value="single frame">
                                    <xs:annotation>
                                        <xs:documentation>Play only a single frame.</xs:documentation>
                                    </xs:annotation>
                                </xs:enumeration>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="firstFrame" type="xs:nonNegativeInteger" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>If symbolType=graphic: First frame to display.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="name" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation>If symbolType &lt;&gt; graphics: Name of the symbol instance.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="blendMode" use="optional">
                        <xs:annotation>
                            <xs:documentation>Blend mode for the symbol instance.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">                                                                                                                        
                                <xs:enumeration value="normal"/>
                                <xs:enumeration value="layer"/>
                                <xs:enumeration value="multiply"/>
                                <xs:enumeration value="screen"/>
                                <xs:enumeration value="lighten"/>
                                <xs:enumeration value="darken"/>
                                <xs:enumeration value="difference"/>
                                <xs:enumeration value="add"/>
                                <xs:enumeration value="subtract"/>
                                <xs:enumeration value="invert"/>
                                <xs:enumeration value="alpha"/>
                                <xs:enumeration value="erase"/>
                                <xs:enumeration value="overlay"/>
                                <xs:enumeration value="hardlight"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="isVisible" type="xs:boolean" default="true">
                        <xs:annotation>
                            <xs:documentation>Visibility of the symbol instance.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="cacheAsBitmap" type="xs:boolean" default="false">
                        <xs:annotation>
                            <xs:documentation>Whether to cache the symbol instance as a bitmap.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="bits32" type="xs:boolean" default="true">
                        <xs:annotation>
                            <xs:documentation>Set to false when the cache as bitmap is opaque.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attributeGroup ref="tns:SelectableAttrs"/>                    
                </xs:complexType>
            </xs:element>
            <xs:element name="DOMBitmapInstance">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="matrix" type="tns:tMatrix">
                            <xs:annotation>
                                <xs:documentation>Transformation matrix for the bitmap instance.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="transformationPoint" type="tns:tPoint" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation point for the bitmap instance.</xs:documentation>
                            </xs:annotation>
                        </xs:element>                                                                                                               
                    </xs:sequence>
                    <xs:attribute name="libraryItemName" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Name of the bitmap in the library.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attributeGroup ref="tns:SelectableAttrs"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="DOMStaticText">
                <xs:annotation>
                    <xs:documentation>Static text element.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tDOMText">                                                  
                            <xs:attributeGroup ref="tns:TextNonInputAttrs"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="DOMDynamicText">
                <xs:annotation>
                    <xs:documentation>Dynamic text element.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tDOMText">                                                  
                            <xs:attributeGroup ref="tns:TextNonInputAttrs"/>
                            <xs:attributeGroup ref="tns:TextNonStaticAttrs"/>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="DOMInputText">
                <xs:annotation>
                    <xs:documentation>Input text element.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tDOMText">                                                  
                            <xs:attributeGroup ref="tns:TextNonStaticAttrs"/>                                                                                                                                                                                      
                            <xs:attribute name="maxCharacters" type="xs:nonNegativeInteger" use="optional">
                                <xs:annotation>
                                    <xs:documentation>Maximum number of characters allowed in the input text field.</xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            
            <xs:element name="DOMShape">
                <xs:annotation>
                    <xs:documentation>Shape element.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="matrix" type="tns:tMatrix" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation matrix for the shape.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="transformationPoint" type="tns:tPoint" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation point for the shape.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="fills" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>List of shape fill styles.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="FillStyle" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:group ref="tns:gFill"/>
                                            <xs:attribute name="index" type="xs:positiveInteger" use="required">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the fill style.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>                                                                                                                                
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="strokes" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>List of shape stroke styles.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="StrokeStyle" maxOccurs="unbounded">                                                                                                                            
                                    <xs:complexType>
                                            <xs:group ref="tns:gStroke"></xs:group>
                                            <xs:attribute name="index" type="xs:positiveInteger" use="required">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the stroke style.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="edges">
                            <xs:annotation>
                                <xs:documentation>List of shape edges.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Edge" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:attribute name="edges" type="xs:string" use="optional">
                                                <xs:annotation>
                                                    <xs:documentation>Edge data in encoded format.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="strokeStyle" type="xs:nonNegativeInteger" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the stroke style to use for this edge. 0 = no stroke.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="fillStyle0" type="xs:nonNegativeInteger" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the fill style to use for the area to the left of the edge. 0 = no fill.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="fillStyle1" type="xs:nonNegativeInteger" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the fill style to use for the area to the right of the edge. 0 = no fill.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="cubics" use="optional">
                                                <xs:annotation>
                                                    <xs:documentation>Cubic curve data in encoded format.</xs:documentation>
                                                </xs:annotation>
                                                <xs:simpleType>
                                                    <xs:restriction base="xs:string">
                                                        <xs:pattern value="!(-?[0-9]+) +(-?[0-9]+) *\(((-?[0-9]+) *, *(-?[0-9]+))? *; *(-?[0-9]+),(-?[0-9]+) +(-?[0-9]+),(-?[0-9]+) +(-?[0-9]+),(-?[0-9]+) *(([QqPp]? *-?[0-9]+ +-?[0-9]+)+) *\)((-?[0-9]+) *, *(-?[0-9]+))? *; *"/>
                                                    </xs:restriction>
                                                </xs:simpleType>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>       
                    <xs:attribute name="isFloating" type="xs:boolean" default="false">
                        <xs:annotation>
                            <xs:documentation>Unsure: Whether the shape is a floating shape.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attributeGroup ref="tns:SelectableAttrs"/>                                                                                                    
                </xs:complexType>
            </xs:element>

            <xs:element name="DOMComponentInstance">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="matrix" type="tns:tMatrix" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation matrix for the component instance.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="transformationPoint" type="tns:tPoint" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Transformation point for the component instance.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dataBindingXML" minOccurs="0" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>XML string containing data binding information.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="parametersAsXML" minOccurs="0" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>XML string containing component parameters.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="libraryItemName" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Name of the library item.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="instanceName" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation>Name of the component instance.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="centerPoint3DX" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>X coordinate of the 3D center point.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="centerPoint3DY" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>Y coordinate of the 3D center point.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="centerPoint3DZ" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>Z coordinate of the 3D center point.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="uniqueID" type="xs:nonNegativeInteger" use="optional">
                        <xs:annotation>
                            <xs:documentation>Unique ID of the component instance.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="tDOMFrame">
        
        <xs:sequence>            
        <xs:element name="Actionscript" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>ActionScript code associated with the frame.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="script" type="xs:string"/>                                                                                                    
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="MorphShape" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Morph shape data for shape tweens.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="morphSegments">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="MorphSegment" minOccurs="0" maxOccurs="unbounded">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="MorphCurves" maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:attribute name="controlPointA" type="xs:string">
                                                            <xs:annotation>
                                                                <xs:documentation>Control point of the curve in morph segment in first shape.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:attribute>
                                                        <xs:attribute name="anchorPointA" type="xs:string">
                                                            <xs:annotation>
                                                                <xs:documentation>Anchor point of the curve in morph segment in first shape.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:attribute>
                                                        <xs:attribute name="controlPointB" type="xs:string">
                                                            <xs:annotation>
                                                                <xs:documentation>Control point of the curve in morph segment in second shape.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:attribute>
                                                        <xs:attribute name="anchorPointB" type="xs:string">
                                                            <xs:annotation>
                                                                <xs:documentation>Anchor point of the curve in morph segment in second shape.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:attribute>
                                                        <xs:attribute name="isLine" type="xs:boolean" use="optional" default="false">
                                                            <xs:annotation>
                                                                <xs:documentation>Whether the curve is a straight line.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:attribute>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="startPointA" type="xs:string" use="required">
                                                <xs:annotation>
                                                    <xs:documentation>Starting point of the curves in morph segment in first shape.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="startPointB" type="xs:string" use="required">
                                                <xs:annotation>
                                                    <xs:documentation>Starting point of the curves in morph segment in second shape.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="strokeIndex1" type="xs:nonNegativeInteger" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the stroke in the first shape.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="strokeIndex2" type="xs:nonNegativeInteger" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the stroke in the second shape.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="fillIndex1" type="xs:nonNegativeInteger" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the fill in the first shape.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="fillIndex2" type="xs:nonNegativeInteger" use="optional" default="0">
                                                <xs:annotation>
                                                    <xs:documentation>Index of the fill in the second shape.</xs:documentation>
                                                </xs:annotation>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="morphHintsList">
                            <xs:annotation>
                                <xs:documentation>Unsure: Unknown purpose and structure.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType/>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="mappingType" default="distributive">
                        <xs:annotation>
                            <xs:documentation>Type of mapping used for the shape tween.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="distributive"/>
                                <xs:enumeration value="angular vertex"/>
                                <xs:enumeration value="random distribution"/>
                                <xs:enumeration value="linear walk"/>
