                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="DOMFolderItem" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:attribute name="name" type="xs:string" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Name of the folder. For subfolders, use a slash (/) in the path. Example: "myFolder" or "mySub/myFolder".</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="itemID" type="tns:tItemId" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Unique identifier for the folder.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="isExpanded" type="xs:boolean" use="optional" default="false">
                                        <xs:annotation>
                                            <xs:documentation>Indicates if the folder is expanded in the Library panel.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="fonts" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Fonts used in the document.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="DOMFontItem" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation>Font used in the document.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:attribute name="name" type="xs:string" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Name of the font. Unique identifier for the font.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="itemID" type="tns:tItemId" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Unique identifier for the font.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="font" type="xs:string" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Font family name.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="size" type="xs:integer" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Font size in points.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="id" type="xs:integer" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Unique identifier for the font item.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="sourceLastImported" type="xs:integer" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Timestamp of the last import of the font source.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="embedRanges" use="optional" default="">
                                        <xs:annotation>
                                            <xs:documentation>Character ranges to embed for the font. Numbers separated by pipes. Example: "1|2|3" for uppercase, lowercase and numerals.</xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:pattern value="([0-9]+)?(\|[0-9]+)*"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="embeddedCharacters" type="xs:string" use="optional" default="">
                                        <xs:annotation>
                                            <xs:documentation>Specific characters that are embedded for the font. Just the characters themselves, without any separators.</xs:documentation>
                                        </xs:annotation>    
                                    </xs:attribute>
                                    <xs:attributeGroup ref="tns:LinkageAttrs"></xs:attributeGroup>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="media" minOccurs="0">
                    <xs:complexType>
                        <xs:choice minOccurs="0" maxOccurs="unbounded">
                            <xs:element name="DOMBitmapItem">
                                <xs:complexType>
                                    <xs:attributeGroup ref="tns:MediaAttrs"/>
                                    <xs:attribute name="useImportedJPEGData" type="xs:boolean" use="optional" default="true">
                                        <xs:annotation>
                                            <xs:documentation>Indicates whether to use the imported JPEG data for the bitmap or recompress it.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="compressionType" type="tns:tBitmapCompressionType" use="optional" default="photo">
                                        <xs:annotation>
                                            <xs:documentation>Compression type used for the bitmap.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute> 
                                    <xs:attribute name="originalCompressionType" type="tns:tBitmapCompressionType" use="optional" default="photo">
                                        <xs:annotation>
                                            <xs:documentation>Original compression type of the bitmap when it was imported.</xs:documentation>
                                        </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="quality" use="required">
                                        <xs:annotation>
                                            <xs:documentation>Quality of the bitmap compression (0-100).</xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                            <xs:restriction base="xs:integer">
                                                <xs:minInclusive value="0"/>
                                                <xs:maxInclusive value="100"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="href" type="xs:string" use="required">
                                        <xs:annotation>
