<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
xmlns:tns="http://ns.adobe.com/xfl/2008/"
targetNamespace="http://ns.adobe.com/xfl/2008/"
elementFormDefault="qualified"
>
    <xs:annotation>
        <xs:documentation xml:lang="en"><![CDATA[
            <h1>XFL Schema</h1>
            <p>Schema describing XFL files used by Adobe Flash Professional CS5 and later.</p>            
            <p>Version: <b>Unreleased</b>, release date (yyyy-mm-dd)</p>
            <p>Author: Jindra Petrik (JPEXS)</p>
            <p>Contact: jindra.petrik@gmail.com</p>
            <p>WARNING: Schema might be incomplete or inaccurate.</p>
            <p>Parts where I really don't know are marked with "Unsure" prefix.</p>
        ]]></xs:documentation>
        <xs:appinfo>
            <meta:version xmlns:meta="urn:metadata">0.0.0</meta:version>
            <meta:releaseDate xmlns:meta="urn:metadata">yyyy-mm-dd</meta:releaseDate>
        </xs:appinfo>
    </xs:annotation>
    <xs:simpleType name="tBitmapCompressionType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="photo">
                <xs:annotation>
                    <xs:documentation>Lossy compression used for photos - JPEG.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="lossless">
                <xs:annotation>
                    <xs:documentation>Lossless compression used for images - like PNG.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tItemId">
        <xs:annotation>
            <xs:documentation>
                Unique item identifier.
                First 8 characters are hexadecimal of timeCreated.
                Last 8 characters seems to be hexadecimal of order of creation this object among all objects.
                Example: "604e0a22-000002e1"
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{8}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tColor">
        <xs:annotation>
            <xs:documentation>
                Color in hexadecimal format: #RRGGBB
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="#[0-9a-fA-F]{6}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="tSymbolType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="movie clip"/>
            <xs:enumeration value="button"/>
            <xs:enumeration value="graphic"/>
            <xs:enumeration value="ik container"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="tMatrix">
        <xs:annotation>
            <xs:documentation>2d graphics placement matrix.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Matrix">
                <xs:complexType>
                    <xs:attribute name="a" type="xs:float" use="optional" default="1">
                        <xs:annotation>
                            <xs:documentation>Scaling in x direction.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="b" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>Skewing in y direction.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="c" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>Skewing in x direction.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="d" type="xs:float" use="optional" default="1">
                        <xs:annotation>
                            <xs:documentation>Scaling in y direction.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="tx" type="xs:float" use="optional" default="0">   
                        <xs:annotation>
                            <xs:documentation>Translation in x direction.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ty" type="xs:float" use="optional" default="0">
                        <xs:annotation>
                            <xs:documentation>Translation in y direction.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>                                                                                                                                                                                                                                                   
        </xs:sequence>                                                                                                                        
    </xs:complexType>    

    <xs:complexType name="tGradientEntry">
        <xs:annotation>
            <xs:documentation>
                One stop of the gradient.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="color" type="tns:tColor" use="optional" default="#000000"/>
        <xs:attribute name="alpha" type="xs:float" use="optional" default="1.0"/>
        <xs:attribute name="ratio" default="0.0">
            <xs:annotation>
                <xs:documentation>Position of stop in the gradient.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:float">
                    <xs:minInclusive value="0.0"/>
                    <xs:maxInclusive value="1.0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
        
    <xs:attributeGroup name="GradientAttrs">
        <xs:annotation>
            <xs:documentation>Common attributes for gradient definitions.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="spreadMethod" use="optional" default="pad">
            <xs:annotation>
                <xs:documentation>Method to use when gradient stops do not cover the entire area.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="pad"/>
                    <xs:enumeration value="reflect"/>
                    <xs:enumeration value="repeat"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="interpolationMethod" use="optional" default="rgb">
            <xs:annotation>
                <xs:documentation>Color interpolation method.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="rgb"/>
                    <xs:enumeration value="linearRGB"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:attributeGroup>

    <xs:complexType name="tGradientFilter">
        <xs:annotation>
            <xs:documentation>Common type for GradientGlowFilter and GradientBevelFilter</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="GradientEntry" type="tns:tGradientEntry" minOccurs="2" maxOccurs="unbounded"/>
        </xs:sequence>        
        <xs:attribute name="blurX" use="optional" default="5.0">
            <xs:annotation>
                <xs:documentation>Horizontal blur amount.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:float">
                    <xs:minInclusive value="0.0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="blurY" use="optional" default="5.0">
            <xs:annotation>
                <xs:documentation>Vertical blur amount.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:float">
                    <xs:minInclusive value="0.0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="strength" use="optional" default="1.0">
            <xs:annotation>
                <xs:documentation>Strength of the filter effect.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:float">
                    <xs:minInclusive value="0.0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="quality" type="xs:positiveInteger" use="optional" default="1">
            <xs:annotation>
                <xs:documentation>Number of times to apply the filter.</xs:documentation>
            </xs:annotation>
        </xs:attribute>        
        <xs:attribute name="angle" type="xs:float" use="optional" default="45.0">
            <xs:annotation>
                <xs:documentation>Angle of the filter effect in degrees.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="distance" type="xs:float" use="optional" default="5.0">
            <xs:annotation>
                <xs:documentation>Distance of the filter effect.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="knockout" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>Knockout mode.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="type" use="optional" default="inner">
            <xs:annotation>
                <xs:documentation>Type of the gradient filter.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="inner"/>
                    <xs:enumeration value="outer"/>
                    <xs:enumeration value="full"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attributeGroup ref="tns:FilterAttrs"/>
    </xs:complexType>

    <xs:group name="gFill">
        <xs:choice>
            <xs:element name="SolidColor">
                <xs:annotation>
                    <xs:documentation>Solid color fill.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="color" type="tns:tColor" use="optional" default="#000000"/>
                    <xs:attribute name="alpha" type="xs:float" use="optional" default="1.0"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="RadialGradient">
                <xs:annotation>
                    <xs:documentation>Radial gradient fill.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="matrix" type="tns:tMatrix" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Transformation matrix for the gradient.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="GradientEntry" type="tns:tGradientEntry" minOccurs="2" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>One stop of the gradient.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attributeGroup ref="tns:GradientAttrs"/>
                    <xs:attribute name="focalPointRatio" use="optional" default="0.0">
                        <xs:annotation>
                            <xs:documentation>Focal point ratio for the radial gradient.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:float">
                                <xs:minInclusive value="-1.0"/>
                                <xs:maxInclusive value="1.0"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>                    
                </xs:complexType>
            </xs:element>
            <xs:element name="LinearGradient">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="matrix" type="tns:tMatrix" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Transformation matrix for the gradient.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="GradientEntry" type="tns:tGradientEntry" minOccurs="2" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>One stop of the gradient.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attributeGroup ref="tns:GradientAttrs"/>
                </xs:complexType>
            </xs:element>
            <xs:element name="BitmapFill">
                <xs:annotation>
                    <xs:documentation>Bitmap fill.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="matrix" type="tns:tMatrix" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Transformation matrix for the bitmap fill.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="bitmapPath" type="xs:string" use="required">
                        <xs:annotation>
                            <xs:documentation>Path to the bitmap resource. Link to DOMDocument / media / name.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="bitmapIsClipped" type="xs:boolean" use="optional" default="true">
                        <xs:annotation>
                            <xs:documentation>Specifies whether the bitmap is clipped to the shape bounds.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:choice>    
    </xs:group>

    <xs:group name="gStroke">
        <xs:annotation>
            <xs:documentation>Different types of strokes.</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="SolidStroke">
                <xs:annotation>
                    <xs:documentation>Solid stroke.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tStroke">
                            <xs:attribute name="caps" default="round">
                                <xs:annotation>
                                    <xs:documentation>Type of line caps.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="none"/>
                                        <xs:enumeration value="round"/>
                                        <xs:enumeration value="square"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="joints" default="round">
                                <xs:annotation>
                                    <xs:documentation>Type of line joints.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="miter">
                                            <xs:annotation>
                                                <xs:documentation>Note: Use miterLimit attribute to specify the limit.</xs:documentation>
                                            </xs:annotation>
                                        </xs:enumeration>
                                        <xs:enumeration value="round"/>
                                        <xs:enumeration value="bevel"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>    
                </xs:complexType>                                                                                                                                               
            </xs:element>
            <xs:element name="DashedStroke">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tStroke">
                            <xs:attribute name="dash1" default="6.0">
                                <xs:annotation>
                                    <xs:documentation>Length of the dash segment in pts.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:float">
                                        <xs:minInclusive value="0.25"/>
                                        <xs:maxInclusive value="300.0"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="dash2" default="6.0">
                                <xs:annotation>
                                    <xs:documentation>Length of the gap segment in pts.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:float">
                                        <xs:minInclusive value="0.25"/>
                                        <xs:maxInclusive value="300.0"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="DottedStroke">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tStroke">
                            <xs:attribute name="dotSpace" default="3.0">
                                <xs:annotation>
                                    <xs:documentation>Dot spacing in pts.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:float">
                                        <xs:minInclusive value="0.0"/>
                                        <xs:maxInclusive value="300.0"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>                                                                                                                                                        
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="RaggedStroke">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tStroke">
                            <xs:attribute name="pattern" default="simple">
                                <xs:annotation>
                                    <xs:documentation>Ragged stroke pattern.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="solid"/>
                                        <xs:enumeration value="simple"/>
                                        <xs:enumeration value="random"/>
                                        <xs:enumeration value="dotted"/>
                                        <xs:enumeration value="random dotted"/>
                                        <xs:enumeration value="triple dotted"/>
                                        <xs:enumeration value="random tripple dotted"/>                                                                                                                                                                    
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>        
                            <xs:attribute name="waveHeight" default="wavy">
                                <xs:annotation>
                                    <xs:documentation>Height of the waves.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="flat"/>
                                        <xs:enumeration value="wavy"/>
                                        <xs:enumeration value="very wavy"/>
                                        <xs:enumeration value="wild"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="waveLength" default="short">
                                <xs:annotation>
                                    <xs:documentation>Length of the waves.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="very short"/>
                                        <xs:enumeration value="short"/>
                                        <xs:enumeration value="medium"/>
                                        <xs:enumeration value="long"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="StippleStroke">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tStroke">
                            <xs:attribute name="dotSize" default="small">
                                <xs:annotation>
                                    <xs:documentation>Size of the stipple dots.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="tiny"/>
                                        <xs:enumeration value="small"/>
                                        <xs:enumeration value="medium"/>
                                        <xs:enumeration value="large"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="variation" default="varied sizes">
                                <xs:annotation>
                                    <xs:documentation>Variation of the stipple dot sizes.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="one size"/>
                                        <xs:enumeration value="small variation"/>
                                        <xs:enumeration value="varied sizes"/>
                                        <xs:enumeration value="random sizes"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>           
                            <xs:attribute name="density" default="sparse">
                                <xs:annotation>
                                    <xs:documentation>Density of the stipple dots.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="very dense"/>
                                        <xs:enumeration value="dense"/>
                                        <xs:enumeration value="sparse"/>
                                        <xs:enumeration value="very sparse"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
                <xs:element name="HatchedStroke">
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="tns:tStroke">
                            <xs:attribute name="hatchThickness" default="hairline">
                                <xs:annotation>
                                    <xs:documentation>Thickness of the hatch lines.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="hairline"/>
                                        <xs:enumeration value="thin"/>
                                        <xs:enumeration value="medium"/>
                                        <xs:enumeration value="thick"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="space" default="distant">
                                <xs:annotation>
                                    <xs:documentation>Spacing between the hatch lines.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="very close"/>
                                        <xs:enumeration value="close"/>
                                        <xs:enumeration value="distant"/>
                                        <xs:enumeration value="very distant"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="jiggle" default="none">
                                <xs:annotation>
                                    <xs:documentation>Jiggle of the hatch lines.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="none"/>
                                        <xs:enumeration value="bounce"/>
                                        <xs:enumeration value="loose"/>
                                        <xs:enumeration value="wild"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="rotate" default="none">
                                <xs:annotation>
                                    <xs:documentation>Rotation of the hatch lines.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="none"/>
                                        <xs:enumeration value="slight"/>
                                        <xs:enumeration value="medium"/>
                                        <xs:enumeration value="free"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="curve" default="straight">
                                <xs:annotation>
                                    <xs:documentation>Curviness of the hatch lines.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="straight"/>
                                        <xs:enumeration value="slight curve"/>
                                        <xs:enumeration value="medium curve"/>
                                        <xs:enumeration value="very curved"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute name="length" default="equal">
                                <xs:annotation>
                                    <xs:documentation>Length variation of the hatch lines.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="equal"/>
                                        <xs:enumeration value="slight variation"/>
                                        <xs:enumeration value="medium variation"/>
                                        <xs:enumeration value="random"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
        </xs:choice>
    </xs:group>

    <xs:complexType name="tStroke">
        <xs:sequence>
            <xs:element name="fill">
                <xs:annotation>
                    <xs:documentation>Determines how the stroke is filled.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:group ref="tns:gFill"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="scaleMode" use="optional" default="none">
            <xs:annotation>
                <xs:documentation>Stroke scaling mode.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="none">
                        <xs:annotation>
                            <xs:documentation>Do not scale strokes.</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="normal">
                        <xs:annotation>
                            <xs:documentation>Scale strokes both vertically and horizontally.</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="vertical">
                        <xs:annotation>
                            <xs:documentation>Scale strokes only vertically.</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="horizontal">
                        <xs:annotation>
                            <xs:documentation>Scale strokes only horizontally.</xs:documentation>
                        </xs:annotation>
                    </xs:enumeration>
                </xs:restriction>
            </xs:simpleType
        ></xs:attribute>
        <xs:attribute name="weight" use="optional" default="1.0">
            <xs:annotation>
                <xs:documentation>Stroke thickness in pixels.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:float">
                    <xs:minInclusive value="0.0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="miterLimit" default="3.0">
            <xs:annotation>
                <xs:documentation>Miter limit of the stroke.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:float">
                    <xs:minInclusive value="1.0"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="pixelHinting" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>Specifies whether to use pixel hinting.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
