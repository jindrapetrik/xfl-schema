                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="SoundEnvelope" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Sound envelope points for sound effects.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SoundEnvelopePoint" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="mark44" type="xs:nonNegativeInteger" default="0">
                                    <xs:annotation>
                                        <xs:documentation>Position of the envelope point in 44kHz samples.</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="level0" default="0">
                                    <xs:annotation>
                                        <xs:documentation>Volume level of the left channel. 0-65535.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:integer">
                                            <xs:minInclusive value="0"/>
                                            <xs:maxInclusive value="65535"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:attribute>
                                <xs:attribute name="level1" default="0">
                                    <xs:annotation>
                                        <xs:documentation>Volume level of the right channel. 0-65535.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:integer">
                                            <xs:minInclusive value="0"/>
                                            <xs:maxInclusive value="65535"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="tweens" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Custom ease curves for the frame.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CustomEase" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Custom ease curve for a specific target property.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Point" minOccurs="2" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>Point defining the custom ease curve.</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:attribute name="x" use="optional" default="0.0">
                                                <xs:annotation>
                                                    <xs:documentation>X coordinate of the point on the ease curve (0.0 to 1.0).</xs:documentation>
                                                </xs:annotation>
                                                <xs:simpleType>
                                                    <xs:restriction base="xs:float">
                                                        <xs:minInclusive value="0.0"/>
                                                        <xs:maxInclusive value="1.0"/>
                                                    </xs:restriction>
                                                </xs:simpleType>
                                            </xs:attribute>
                                            <xs:attribute name="y" use="optional" default="0.0">
                                                <xs:annotation>
                                                    <xs:documentation>Y coordinate of the point on the ease curve (0.0 to 1.0).</xs:documentation>
                                                </xs:annotation>
                                                <xs:simpleType>
                                                    <xs:restriction base="xs:float">
                                                        <xs:minInclusive value="0.0"/>
                                                        <xs:maxInclusive value="1.0"/>
                                                    </xs:restriction>
                                                </xs:simpleType>
                                            </xs:attribute>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                                <xs:attribute name="target" use="required">
                                    <xs:annotation>
                                        <xs:documentation>Target property for which the custom ease curve is defined.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:enumeration value="position"/>
                                            <xs:enumeration value="rotation"/>
                                            <xs:enumeration value="scale"/>
                                            <xs:enumeration value="color"/>
                                            <xs:enumeration value="filters"/>
                                            <xs:enumeration value="all">
                                                <xs:annotation>
                                                    <xs:documentation>Applies the custom ease curve to all properties.</xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:attribute>
                            </xs:complexType>                            
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="elements" maxOccurs="1" type="tns:tElementList" />

        </xs:sequence>
        
        <xs:attribute name="index" type="xs:integer" use="required">
            <xs:annotation>
                <xs:documentation>Index of the frame within the layer. (0-based)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="duration" type="xs:integer" use="optional" default="1">
            <xs:annotation>
                <xs:documentation>Duration of the frame in frames.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="keyMode" type="xs:integer" use="required">
            <xs:annotation>
                <xs:documentation><![CDATA[
        <h1>
            Unsure: Key mode flags for the frame.
        </h1>
        <p>
        The value seems to be a bitmask that defines various properties of the frame's tweening behavior.
        </p>
        The following are the possible key mode values:

        <h2>tweenType = "none":</h2>
        <p>
        0x2600 (9728 dec)
        </p>
        <h2>tweenType = "motion":</h2>
        <p>
        0x4001 +<br />
        <table>
            <tr><td>0x0100</td><td>motionTweenOrientToPath = true</td></tr>
            <tr><td>0x0200</td><td>motionTweenScale = true</td></tr>
            <tr><td>0x0400</td><td>motionTweenRotate &lt;&gt; none</td></tr>
            <tr><td>0x0800</td><td>motionTweenSync = true</td></tr>
            <tr><td>0x1000</td><td>motionTweenSnap = true</td></tr>
        </table>
        (default: only motionTweenSnap = true)
        </p>

        <h2>tweenType = "shape":</h2>
        <p>
        0x4602 (17922 dec)
        </p>

        <h2>tweenType = "motion object":</h2>
        <p>
        0x2003 + 0x0800	motionTweenSync "Sync graphic symbols"
        </p>
        <p>
        The second byte seems to be tween type index.
        </p>
                    ]]>
                </xs:documentation>
            </xs:annotation>        
        </xs:attribute>

        <xs:attribute name="tweenType" use="optional" default="none">
            <xs:annotation>
                <xs:documentation>Type of tween applied to the frame.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="none"/>
                    <xs:enumeration value="motion"/>
                    <xs:enumeration value="shape"/>
                    <xs:enumeration value="motion object"/>
                    <xs:enumeration value="IK pose"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="shapeTweenBlend" use="optional" default="distributive">
            <xs:annotation>
                <xs:documentation>Type of blending used for shape tweens.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="distributive"/>
                    <xs:enumeration value="angular"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="acceleration" use="optional" default="0">
            <xs:annotation>
                <xs:documentation>Easing value for tweens. 
                    Range: -100 to 100. 
                    -100 here = "100 out" in IDE, 
                    100 here = "-100 in" in IDE.
                    0 = no easing.
                </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:integer">
                    <xs:minInclusive value="-100"/>
                    <xs:maxInclusive value="100"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="motionTweenSnap" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>Whether to snap the motion tween.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="motionTweenOrientToPath" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>Whether the motion tween is oriented to the path.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="motionTweenSync" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>Whether the motion tween is synchronized with the timeline.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="motionTweenRotate" default="none">
            <xs:annotation>
                <xs:documentation>Rotation behavior for the motion tween.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="none"/>
                    <xs:enumeration value="auto"/>
                    <xs:enumeration value="clockwise"/>
                    <xs:enumeration value="counter-clockwise"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="motionTweenRotateTimes" type="xs:nonNegativeInteger" use="optional" default="0">
            <xs:annotation>
                <xs:documentation>Number of times to rotate during the motion tween.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="useSingleEaseCurve" type="xs:boolean" use="optional" default="true">
            <xs:annotation>
                <xs:documentation>Whether to use a single ease curve for all properties.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="hasCustomEase" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
                <xs:documentation>Whether the frame has a custom ease curve.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="soundName" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Name of the sound associated with the frame.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="soundLoop" type="xs:integer" use="optional" default="1">
            <xs:annotation>
                <xs:documentation>Number of times the sound should loop.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="soundLoopMode" default="repeat">
            <xs:annotation>
                <xs:documentation>Loop mode for the sound.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="repeat"/>
                    <xs:enumeration value="loop"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="inPoint44" type="xs:integer" use="optional" default="0">
            <xs:annotation>
                <xs:documentation>Point in time for sound to start. Measured in 44kHz samples.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="outPoint44" type="xs:integer" use="optional" default="0">
            <xs:annotation>
                <xs:documentation>Point in time for sound to stop. Measured in 44kHz samples.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="soundZoomLevel" type="xs:integer" use="optional">
            <xs:annotation>
                <xs:documentation>Zoom level for the sound.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="soundEffect" default="none">
            <xs:annotation>
                <xs:documentation>Sound effect applied to the sound.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="none"/>
                    <xs:enumeration value="left channel"/>
                    <xs:enumeration value="right channel"/>
                    <xs:enumeration value="fade left to right"/>
                    <xs:enumeration value="fade right to left"/>
                    <xs:enumeration value="fade in"/>
                    <xs:enumeration value="fade out"/>
                    <xs:enumeration value="custom"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="soundSync" default="event">
            <xs:annotation>
                <xs:documentation>Sound synchronization mode.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="event"/>
                    <xs:enumeration value="start"/>
                    <xs:enumeration value="stop"/>
                    <xs:enumeration value="stream"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="name" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Name of the frame.</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="labelType" default="none">
            <xs:annotation>
                <xs:documentation>Type of label for the frame.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="none"/>
                    <xs:enumeration value="name"/>
                    <xs:enumeration value="comment"/>
                    <xs:enumeration value="anchor"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    
    <xs:complexType name="tDOMTimeline">
        <xs:sequence>
            <xs:element name="layers">
                <xs:annotation>
                    <xs:documentation>Layers in the timeline.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="DOMLayer" minOccurs="0" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Layer in the timeline.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="frames" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Frames in the layer.</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="DOMFrame" minOccurs="0" maxOccurs="unbounded" type="tns:tDOMFrame">
                                                    <xs:annotation>
                                                        <xs:documentation>Frame in the layer.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>

                                <xs:attribute name="name" type="xs:string" use="required">
                                    <xs:annotation>
                                        <xs:documentation>Name of the layer.</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="color" type="tns:tColor" use="required">
                                    <xs:annotation>
                                        <xs:documentation>Color associated with the layer.</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="current" type="xs:boolean" use="optional" default="false">
                                    <xs:annotation>
                                        <xs:documentation>Indicates if the layer is currently active.</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="isSelected" type="xs:boolean" use="optional" default="false">
                                    <xs:annotation>
                                        <xs:documentation>Indicates if the layer is selected.</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="locked" type="xs:boolean" use="optional" default="false">
                                    <xs:annotation>
                                        <xs:documentation>Indicates if the layer is locked.</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="autoNamed" type="xs:boolean" use="optional" default="true">
                                    <xs:annotation>
                                        <xs:documentation>Indicates if the layer was auto-named and has not been renamed by the user.</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="layerType" use="optional" default="normal">
                                    <xs:annotation>
                                        <xs:documentation>Type of the layer.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
