    </xs:complexType>

    <xs:element name="DOMDocument">
        <xs:annotation>
            <xs:documentation>Main document element.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="folders" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Folders in the Library.</xs:documentation>
